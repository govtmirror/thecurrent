.tooltip {
  color: #000000; 
  outline: none;
  cursor: help; 
  text-decoration: none;
  position: relative;
}
.tooltip span {
  margin-left: -999em !important;
  position: absolute;
}


.tooltip:hover span {
  font-family: Calibri, Tahoma, Geneva, sans-serif;
  position: absolute;
  left: 1em;
  top: 2em;
  margin-left: 0  !important;
  width: 250px;
}
.tooltip:hover img {
  border: 0;
  margin: -10px 0 0 -55px;
  float: left;
  position: absolute;
}
.tooltip:hover em {
  font-family: Candara, Tahoma, Geneva, sans-serif;
  font-size: 1.2em;
  font-weight: bold;
  display: block;
  padding: 0.2em 0 0.6em 0;
}
